<h2>Your Routines:</h2>
<% if @routines.empty? %>
    <p> You have no routines yet! </p>
<% else %>
    <ul>
    <% @routines.each do |routine| %>
        <li><%= link_to "#{routine.title}" %></li>
    <% end %>
    </ul>
<% end %>

<h2>Your Workouts:</h2>
<% if current_user.workouts.empty? %>
    <p> You have no workouts yet! </p>
<% else %>
    <ul>
    <% current_user.workouts.each do |workout| %>
        <li><%= link_to workout.name, workout_path(workout) %></li>
   <% end %>
    </ul>
<% end %>

<%= link_to "Start New Routine", new_user_routine_path(current_user), class:"navbar-brand" %> <br/>
<%= link_to "See All Available Workouts", workouts_path %> <br/>
Or
<%= link_to "Add it", new_workout_path %> <br/>