<%= form_for [user, routine] do |f| %>
    <%= f.label :title %><br/>
    <%= f.text_field :title, value: "Routine XX" %><br />
    <%= f.label :date %><br />
    <%= f.date_select :date %><br />
    <%= f.label :workout %><br />
    <%= f.collection_select :workout_id, Workout.all, :id, :name, :prompt => true%><br/>
    <br/>
    <%= f.submit %>
   
<% end %>