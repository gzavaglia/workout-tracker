<%= form_for [user, routine] do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, value: "Routine XX" %><br />
    <%= f.label :date %>
    <%= f.date_select :date %><br />
    <%= f.label :workout %>
    <%= f.collection_select :workout_id, Workout.all, :id, :name %>
    <%= f.submit %>
   
<% end %>